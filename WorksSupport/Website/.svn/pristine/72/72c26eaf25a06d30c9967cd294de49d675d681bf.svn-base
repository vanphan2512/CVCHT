<h2>
    {{i18n.configurations.member.title}}
</h2>
<check-status></check-status>
<div class="main-tab-content margin-top-15">
    <div class="nav-control">
        <div class="row">
            <div class="col-lg-6">
                <div class="main-buttons">
                    <button type="button" id="addNewMember" (click)="onAddNewMemberClick($event)" *ngIf="isCanAdd" class="btn-eof blue-bg m-right-10"
                        data-toggle="modal" data-target="#modalUpdate">
                        <i class="fa fa-plus" aria-hidden="true"></i>
                    </button>

                    <button type="button" id="btnDeletePopup" *ngIf="isCanDelete" (click)="onShowPopupDelete($event)" class="btn-eof red-bg m-right-10"
                        data-toggle="modal" data-target="#modal">
                        <i class="fa fa-trash-o" aria-hidden="true"></i>
                    </button>
                    <div class="main-search" id="searchButton">
                        <div class="input-group">
                            <input type="text" id="memberName" name="memberName" class="form-control txtInputSearch" placeholder="Nhập từ khóa tìm kiếm"
                                aria-describedby="basic-addon2">
                            <div class="input-group-addon" id="searchAddon" (click)="SearchByKey($event)">
                                <button type="button">
                                <i class="fa fa-search" aria-hidden="true"></i>
                            </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="tab-filter">
                    <div>
                        <div class="custom-select"><i class="fa fa-filter" aria-hidden="true"></i> Lọc theo:
                            <select class="form-control" (change)="onStatusChange($event)">
                                <option value="0">Chưa xóa</option>
                                <option value="1">Đã xóa</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12" id="delFail" >
            <grid [eventKey]="model.eventKey" (onToggleSelectChanged)="onToggleSelectChanged($event)" (onIconSelectChanged)="onIconSelectChanged($event)"
                [options]="model.options" (onItemEditClicked)="onEditMemberClicked($event)"></grid>
        </div>
       
    </div>
</div>
<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modal">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <div *ngIf='selectedValue?.length > 0'>
                <h2 class="modal-title text-center">
                    Bạn có chắc chắn muốn xóa!

                </h2>
            </div>
            <div *ngIf='selectedValue?.length == 0'>
                <h2 class="modal-title text-center">
                    Vui lòng chọn dữ liệu để xóa!
                </h2>
            </div>
            <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
            <div class="modal-body">
                <ul class="list-lg">
                    <li *ngFor="let member of selectedValue">
                        <!--<span *ngIf="member.isSystem == true">{{member.name}} -- {{member.isSystem}} -- {{userLogin}} -- {{roleValue}} </span>-->
                        {{member.name}}
                    </li>
                </ul>
            </div>
            <div *ngIf='selectedValue?.length > 0' style="margin-top: 20px">
                <div class="text-center">
                    <button type="button" class="btn btn-eoffice red-bg" (click)="onDeleteMembers($event)"><i class="fa fa-trash-o" aria-hidden="true"></i> Xóa</button>
                    <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i> {{i18n.common.form.cancelDel}}</button>
                </div>
            </div>
            <div *ngIf='selectedValue?.length == 0' style="margin-top: 20px">
                <div class="text-center">
                    <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i> {{i18n.common.form.cancelDel}}</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!--Show popup add  or edit-->
<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modalUpdate">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">

            <!--adding title  -->
            <h2 class="modal-title" *ngIf="isAddPopup">
                <i class="fa fa-plus-square" aria-hidden="true"></i> {{i18n.configurations.addMember.title}}
            </h2>

            <!--editting title  -->
            <h2 class="modal-title" *ngIf="!isAddPopup">
                <i class="fa fa-pencil-square" aria-hidden="true"></i> {{i18n.configurations.updateMember.title}}
            </h2>

            <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
            <div class="modal-body">
                <div>
                    <form-default>
                        <div>
                            <error-message></error-message>
                            
                             <div class="form-group"  *ngIf="!isAddPopup">
                                <label class="control-label col-md-3 col-sm-6 col-xs-12">{{i18n.configurations.updateQuanlity.orderIndex}}<span style = "color:red"> *</span></label>
                                <div class="col-md-9 col-sm-6 col-xs-12">
                                    <form-number-input name="OrderIndexEdit" id="OrderIndex" [(model)]=OrderIndex (keyup)="clearValidateInput('OrderIndex')"></form-number-input>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-6 col-xs-12">{{i18n.configurations.addMember.name}}<span style = "color:red"> *</span></label>
                                <div class="col-md-9 col-sm-6 col-xs-12">
                                    <form-text-input name="WorksSupportMemberRoleNameAdd" id="WorksSupportMemberRoleName" [placeHolderText]=i18n.configurations.addMember.inputName
                                        [validation]="['configurations.addMember.validation.nameIsRequire','configurations.addMember.validation.keyAlreadyExisted']"
                                        [(model)]=WorksSupportMemberRoleName (keyup)="clearValidateInput('WorksSupportMemberRoleName')">
                                    </form-text-input>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-6 col-xs-12">{{i18n.configurations.addMember.desc}}</label>
                                <div class="col-md-9 col-sm-6 col-xs-12">
                                    <form-textarea [placeHolderText]=i18n.configurations.addMember.inputDesc [(model)]=Description id="Description" (keyup)="clearValidateInput('Description')">
                                    </form-textarea>
                                </div>
                            </div>

                            <!-- inputIconUrl -->
                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name"> {{i18n.configurations.addMember.iconUrl}} </label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <i class="{{IconUrl}} fa-lg"></i>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name"></label>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <button type="button" class="btn btn-eoffice green-bg" data-toggle="modal" data-target="#myIconModal" (click)="onIconSelectedEdit()"><i class="fa fa-image" aria-hidden="true"></i> {{i18n.configurations.addMember.inputIconUrl}}</button>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name"></label>
                                <div class="col-md-9 col-sm-9 col-xs-12">
                                    <div class="row">
                                        <div class="col-md-4 col-sm-4 col-xs-12">
                                            <input [disabled]="!isAdmin" class="custom-checkbox" id="IsSystem" [checked]="IsSystem" (change)="IsSystem = !IsSystem" type="checkbox">
                                            <label style="vertical-align: super;" for="IsSystem">{{i18n.configurations.addMember.system}}</label>
                                        </div>
                                        <div class="col-md-4 col-sm-4 col-xs-12">
                                            <span class="ios-tog ios-tog">
                                                <input id="IsActived" [checked]="IsActived"  id="togglyIsActived"  (change)="IsActived = !IsActived" type="checkbox">
                                                <label for="togglyIsActived"><i></i></label>
                                            </span>
                                            <label for="first-name" class="lblIsActive">{{i18n.configurations.addMember.actived}}</label>

                                        </div>
                                        <div class="col-md-4 col-sm-4 col-xs-12">

                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group text-center">
                                <button id="save" *ngIf="isAddPopup" (click)="onAddClicked($event)" type="button" class="btn btn-eoffice green-bg"><i class="fa fa-floppy-o" aria-hidden="true"></i> {{i18n.common.form.save}}</button>
                                <button id="save" *ngIf="!isAddPopup" (click)="onEditMember($event)" type="button" class="btn btn-eoffice green-bg"><i class="fa fa-floppy-o" aria-hidden="true"></i> {{i18n.common.form.save}}</button>
                                <button id="cancel" (click)="onCancelAdd($event)" type="button" class="btn btn-eoffice white-bg m-right-10"><i class="fa fa-times" aria-hidden="true"></i> {{i18n.common.form.cancel}}</button>
                            </div>
                        </div>
                    </form-default>
                </div>

            </div>
        </div>
    </div>
</div>

