.custom-checkbox {
    cursor: pointer;
    box-sizing: border-box;
    padding: 0;
    width: 20px;
    height: 20px;
    border: none;
    border-radius: 0;
    background: transparent;
    -moz-appearance: checkbox;
    -webkit-appearance: checkbox;
    position:absolute;opacity:0;
}
.custom-checkbox:focus+label:before {
    border: 1px solid #c5c3c6;
}
.custom-checkbox+label:before {
    -webkit-transition: all .1s ease-in-out;
    transition: all .1s ease-in-out;
    content: "";
    position: absolute;
    top: -3px;
    left: 3px;
    z-index: 1;
    width: 14px;
    height: 14px;
    border: 1px solid #fff;
    box-sizing: border-box;
    bottom: 0;
    margin-bottom: auto;
    margin-top: auto;
}
.custom-checkbox+label { position:relative;cursor:pointer;padding:0; padding-left:20px;margin-left:0px; }
.custom-checkbox:checked+label:before {
    -ms-transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    height: 9px;
    border-color: #0288d1;
    border-width: 2px;
    border-top-style: none;
    border-right-style: none
}
.custom-checkbox:not(:checked)+label:before {
    -webkit-transition: all .1s ease-in-out;
    transition: all .1s ease-in-out;
    content: "";
    position: absolute;
    top: 0px;
    left: 3px;
    z-index: 1;
    width: 14px;
    height: 14px;
    border: 1px solid #fff;
    box-sizing: border-box
}
.custom-checkbox:focus+label:before {
    border-color: #0288d1;
}
.custom-checkbox:not(:checked):focus+label:before { border-color: #0288d1; }
.custom-checkbox+label:after{
    content: "";
    position: absolute;
    top: 0px;
    left: 0;
    width: 20px;
    height: 20px;
    background: #fff;
    cursor: pointer;
    box-sizing: border-box;
    -webkit-box-shadow: inset 0px 0px 3px 0px rgba(0,0,0,0.4);
    -moz-box-shadow: inset 0px 0px 3px 0px rgba(0,0,0,0.4);
    box-shadow: inset 0px 0px 3px 0px rgba(0,0,0,0.4);
    margin-top: auto;
    margin-bottom: auto;
    bottom: 0;
}
.custom-checkbox + label {
    margin-left:5px;vertical-align:middle;
}
.custom-checkbox:checked + label {
    font-weight:bold;
}
label>.custom-checkbox { margin-right:5px;margin-top:0; }
.custom-checkbox:disabled { opacity:0.5;pointer-events:none; }
.custom-checkbox:disabled + label { pointer-events:none; }

.label-mar-right .custom-checkbox { margin-top:0;vertical-align:text-bottom; }

