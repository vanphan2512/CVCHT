.valign { display:inline-block;vertical-align:middle; }
label { font-weight:normal; }
.lb-form { margin:5px 0; }
.custom-select .form-control { display:inline;width:auto; }
.row-reset { margin-right: 0px; margin-left: 0px; }
.row-reset>[class*="col-"] {padding-right: 0px;padding-left: 0px;}
.row-short { margin-right: -7px ; margin-left: -7px; }
.row-short>[class*="col-"] {padding-right: 7px;padding-left: 7px;}
.dp-table { display:table;width:100%; }
.dp-row { display:table-row; }
.dp-cell { display:table-cell;vertical-align:top; }
.text-info { color:#ffba62; }
.text-danger { color:#dd2c00; }
.text-success { color:#4caf50; }
#top-bar {
    background: #4c5c68;
    height: 50px;
    border: 0px;
    border-radius: 0;
    margin-bottom: 10px;
}
#top-bar .navbar-nav > li > a {
    color: #c5c3c6;
    font-family: 'Roboto Condensed', sans-serif;
    font-size: 14px;
    text-shadow: none;
    background: url(../images/top-menu-bg.png) no-repeat;
    background-position: center 200%;
    padding: 18px 25px 12px;
}
#top-bar .navbar-nav > li > a:hover, #top-bar .navbar-nav > li > a.active {
    color: #fff;
    background-position: center 100%;
}
#top-bar .navbar-nav > li.dropdown > a {
    padding-left: 50px;
    background-image: none;
}
#top-bar .navbar-nav > li.dropdown > a:hover {
    -ms-transform: translateY(0px);
    -webkit-transform: translateY(0px);
    transform: translateY(0px);
}
#top-bar .navbar-nav > li.dropdown > a {
    margin-top: 0;
}
#top-bar .navbar-nav > li.open > a {
    background: rgba(0, 0, 0, 0.3);
}
#top-bar .navbar-nav > li.open > ul {
    border: 0px;
}
#top-bar .navbar-nav > li.open > ul a {
    color: #0288d1;
    margin: 5px 0;
}
#top-bar .navbar-nav > li.open > ul a i {
    width: 25px;
    text-align: center;
}
#top-bar .navbar-nav > li > .dropdown-menu > li > a {
    color: #0288d1;
}
#top-bar .navbar-nav > li.open > ul a:hover {
    background: none;
    text-decoration: underline;
}
#top-logo { padding:15px 15px; }
