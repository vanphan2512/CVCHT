.tab-left {
    border-radius: 5px;
    background: #eee;
    position: relative;
    height: 100%;
    padding: 15px;
}
.tab-left .nav-stacked > li > a {
    border-radius: 0;
    transition: 0s;
}
.tab-left .nav-stacked > li > a:hover {
    background: #4c5c68;
    color: #fff;
}
.tab-left .nav-stacked > li.active > a {
    background: #0288d1;
    color: #fff;
}
.tab-left .nav-stacked > li > a {
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    white-space: nowrap;
    cursor: pointer;
}
.tab-left .nav-stacked > li.active > a:after, .tab-left .nav-stacked > li.active > a:hover:after {
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 18.5px 0 18.5px 25px;
    border-color: transparent transparent transparent #0288d1;
    content: "";
    position: absolute;
    left: 100%;
    top:0;
}
.tab-left .nav-stacked > li > a:hover:after {
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 18.5px 0 18.5px 25px;
    border-color: transparent transparent transparent #4c5c68;
    content: "";
    position: absolute;
    left: 100%;
    top:0;
}
