                                                            <span class="btn-detail-right"><i class="fa fa-trash-o" aria-hidden="true"  (click)="removeWorkGroupMember(member.UserName)"></i></span>
                                                        </div>
                                                        <div class="avatar-circle">
                                                            <img src="http://eofficecnd.nc.com/album/{{member.DefaultPictureUrl}}" *ngIf="member.DefaultPictureUrl !== null" alt="{{member.FullName}}">
                                                            <img src="app/resources/images/avatarDefault.png" *ngIf="member.DefaultPictureUrl === null" alt="{{member.FullName}}">
                                                        </div>
                                                        <div class="member-info">
                                                            <span class="member-name">{{member.FullName}}</span>
                                                            <span class="member-department">{{member.DepartmentName}}</span>
                                                        </div>
                                                        <select [disabled]="!RoleOfWorkGroup.IsCanEditWorksSupportGroup && !isAdmin" class="form-control" #memberRoleAll (change)="changeMemberRole(memberRoleAll.value, member.UserName, member.WorksSupportMemberRoleId, memberRoleAll.text)">                                                            
                                                             <option *ngFor="let obj of LstMemberRole"  
                                                              value={{obj.WorksSupportMemberRoleId}} [attr.selected]="obj.WorksSupportMemberRoleId == member.WorksSupportMemberRoleId ? true : null" >  
                                                              {{obj.WorksSupportMemberRoleName}}
                                                             </option>
                                                    </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
