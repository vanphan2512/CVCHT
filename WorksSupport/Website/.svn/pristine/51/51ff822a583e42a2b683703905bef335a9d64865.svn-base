<div class="main-content">
    <h2>{{wGName}}</h2>
    <div class="main-tab-content label-mar-right" style="margin-top:0; padding:15px 30px;margin-left: 0;margin-right: 0;">
        <div class="nav-control">
            <div class="row">
                <div class="col-lg-12 text-right">
                    <div class="main-buttons">
                        <div class="main-search  m-right-10" id="searchButton">
                            <div class="input-group">
                                <input type="text" id="worksName" name="worksName" class="form-control" placeholder="Nhập từ khóa tìm kiếm" aria-describedby="basic-addon2"
                                    (keyup)="onSearch()">
                                <span class="input-group-addon" id="searchAddon"><button type="button" (click)="onSearch()"><i class="fa fa-search" aria-hidden="true"></i></button></span>
                            </div>
                        </div>
                        <button type="button" data-toggle="modal" (click)="onShowPopupEdit()" data-target="#modalUpdateWorkGroup" class="btn-eof blue-bg m-right-10 btn-user"><i class="fa fa-user" aria-hidden="true"></i></button>
                        <button type="button" *ngIf="isAdmin || isCanAdd" class="btn-eof blue-bg" (click)="onAddNewWorksClick()"><i class="fa fa-plus" aria-hidden="true"></i></button>
                    </div>
                </div>

            </div>
        </div>
        <div class="row row-short itemWorksGroupDetail">
            <div class="col-lg-2 col-md-3 group-sidebar-left">
                <ul class="iconProjectLeft">
                    <li *ngFor="let project of listProject" [ngClass]="{'active': selectedProjectID.toString() === project.WorksSupportProjectId.toString(), '' : selectedProjectID !== project.WorksSupportProjectId.toString()}"
                        data-toggle="tooltip" (click)="onProjectChange(project.WorksSupportProjectId)">
                        <i class="fa {{project.IconUrl}}" aria-hidden="true"></i> <span class="group-sidebar-left-title">{{project.WorksSupportProjectName}}</span>

                    </li>

                </ul>

                <ul class="works-groups-list-left">
                    <li class="list-group" title="" *ngFor="let worksGroup of listWorksGroup" [ngClass]="{'active': selectedWorksGroupID.toString() === worksGroup.WorksSupportGroupId.toString(), '' : selectedWorksGroupID !== worksGroup.WorksSupportGroupId.toString()}"
                        (click)="onWorksGroupChange(worksGroup.WorksSupportGroupId)">
                        <a>
                            <i class="fa {{worksGroup.IconUrl}}" aria-hidden="true"></i> {{worksGroup.WorksSupportGroupName}}
                        </a>
                    </li>
                </ul>
            </div>
            <div class="col-lg-10 col-md-9">
                <div class="navTop">
                    <ul>
                        <li style="cursor:pointer" (click)="onProjectChange(selectedProjectID)"><i class="fa {{projectIcon}}" aria-hidden="true"></i> {{projectName}}</li>
                        <span class="btn-angle-right"><i class="fa fa-angle-right" aria-hidden="true"></i></span>
                        <li><i class="fa {{wGIcon}}" aria-hidden="true"></i> {{wGName}}</li>
                    </ul>
                </div>
                <div class="lstWorksGroup">
                    <ul class="nav nav-tabs tabs-page" role="tablist">
                        <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">Danh sách công việc</a></li>
                    </ul>
                    <div class="row lstDetailGroup">
                        <table class="table-hover" id="dataTable" style="width:100%">
                            <thead>
                                <th colspan="2" valign="middle" style="text-align:left;width: 42%">Tên công việc</th>
                                <th valign="middle" style="width: 10%">Ngày tạo</th>
                                <th valign="middle" style="width:21%">Tiến độ</th>
                                <th valign="middle" style="width:12%">Trạng thái</th>
                                <th valign="middle" style="width:10%">Xử lý</th>
                            </thead>
                            <tbody>
                                <tr *ngIf="listWorks===null || listWorks.length===0">
                                    <td colspan="6" class="text-center">
                                        Không có công việc
                                    </td>
                                </tr>
                                <tr *ngFor="let works of listWorks">
                                    <td width="5%" valign="middle" align="center">
                                        <div class="dropdown dropdown-left">
                                            <a class="dropdown-toggle btn" href="#" data-toggle="dropdown" id="menu1" (disabled)="isAdmin || isCanEdit || isCanDel" aria-expanded="true"><i class="fa fa-ellipsis-h"></i></a>
                                            <ul class="dropdown-menu " role="menu" aria-labelledby="menu1">
                                                <form>
                                                    <li>
                                                        <button type="button " class="btn dark-bg-drop" *ngIf="isAdmin || isCanEdit" (click)="onEditWorksClick(works.WorksSupportId)"><i class="fa fa-pencil" aria-hidden="true"></i> Chỉnh sửa</button>
                                                    </li>
                                                    <li>
                                                        <button type="button" class="btn dark-bg-drop" *ngIf="isAdmin || isCanDel" (click)="confirmDelete(works.WorksSupportId,works.WorksSupportName)"><i class="fa fa-trash-o" aria-hidden="true"></i> Xóa</button>
                                                    </li>
                                                </form>
                                            </ul>
                                        </div>
                                    </td>
                                    <td valign="middle" align="left"><span (click)="redirecttoDetail(works.WorksSupportId)"><a class="title-works-support-name">{{works.WorksSupportName}}</a></span></td>
                                    <td valign="middle" align="center" style="text-align:center;"><span style="color: #90a4ae;">{{stringAsDate(works.CreatedDate)}}</span></td>
                                    <td valign="middle" align="center" style="text-align:left;">
                                        <div class="progress">
                                            <div class="progress-bar" role="progressbar" [attr.aria-valuenow]="works.Currentprogress" aria-valuemin="0" aria-valuemax="100"
                                                [ngStyle]="{'width': works.Currentprogress + '%'}">
                                                <span class="sr-only">{{works.Currentprogress}}% hoàn thành</span>
                                            </div>
                                        </div>
                                    </td>
                                    <td valign="middle" align="center"><span class="btn btn-eoffice-status max-text-status">{{works.WorksSupportStatusName}}</span></td>
                                    <td valign="middle" align="center">
                                        <div class="dropdown">
                                            <a class="dropdown-toggle btn btn-eoffice-handling blue-bg" href="#" data-toggle="dropdown" [attr.data-id]="works.WorksSupportId"
                                                (click)="onXuLyClick(works.WorksSupportId)" *ngIf="isAdmin || works.IsCanProcessWorkFlow"><i class="fa fa-edit"></i> Xử lý <i class="fa fa-caret-down" aria-hidden="true"></i></a>
                                            <div class="dropdown-menu dropdown-menu-handling" style="padding: 15px; padding-bottom: 10px;">
                                                <form (ngSubmit)="onProcessClicked(works.WorksSupportId)">
                                                    <select class="form-control" required [(ngModel)]="selectedStepID" (ngModelChange)="getStepUser(works.WorksSupportId)">
                                                       <option value="">---Chọn bước xử lý---</option>
                                                        <option id="{{step.NextWorksSupportStepId}}" *ngFor="let step of listWorksFlow" [value]="step.NextWorksSupportStepId">
                                            {{step.WorksSupportStepName}}</option>
                                                    </select>
                                                    <div class="input-group">
                                                        <span class="input-group-addon" id="sizing-addon1"><i class="fa fa-user" aria-hidden="true"></i></span>
                                                        <select class="form-control" [required]="mustChooseNextStepUser" [(ngModel)]="selectedUser" [disabled]="selectedStepID===''">
                                                       <option value="">---Chọn người xử lý---</option>
                                                            <option id="{{user.FullName}}" *ngFor="let user of listUser" [value]="user.UserName">
                                            {{user.FullName}}</option>
                                                    </select>
                                                    </div>
                                                    <div class="input-group">
                                                        <textarea class="form-control" placeholder="Ghi chú..." [disabled]="selectedStepID===''" [(ngModel)]="Note"></textarea>
                                                    </div>
                                                    <button type="submit" class="btn blue-bg" [disabled]="selectedStepID===''">Xử lý</button>
                                                </form>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <div id="noRowBottom" class="bottom">
                            <div class="dataTables_length" id="dataTable_length">
                                <label>
                                                <div class="custom-select">Hiển thị: 
                                                    <select class="form-control" [(ngModel)]="size" (ngModelChange)="onPageSizeChange()">
                                                        <option value=10>10</option>
                                                        <option value=20>20</option>
                                                        <option value=50>50</option>
                                                    </select> dòng / trang
                                                </div>
                                            </label>
                            </div>
                            <div class="dataTables_paginate paging_simple_numbers" id="dataTable_paginate">
                                <ul *ngIf="pager.pages && pager.pages.length" class="pagination">
                                    <li *ngIf="pager.currentPage !== 1">
                                        <a (click)="onPageChange(pager.currentPage - 1)"><i class="fa fa-angle-double-left" aria-hidden="true"></i></a>
                                    </li>
                                    <li *ngFor="let page of pager.pages" [ngClass]="{active:pager.currentPage === page}">
                                        <a (click)="onPageChange(page)">{{page}}</a>
                                    </li>
                                    <li *ngIf="pager.currentPage !== pager.totalPages">
                                        <a (click)="onPageChange(pager.currentPage + 1)"><i class="fa fa-angle-double-right" aria-hidden="true"></i></a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modal-add-works-group-related">
                <div class="modal-dialog modal-lg popupTab" role="document">
                    <div class="modal-content">
                        <div class="titleProject">
                            <h2 class="modal-title">
                                <i class="fa fa-plus-square" aria-hidden="true"></i> Thêm công việc liên quan
                            </h2>
                        </div>
                        <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
                        <div class="modal-body tab-scroll" style="padding: 20px;">
                            <div class="row row-short">

                                <div class="col-lg-2 col-xs-12">
                                    <select class="form-control" [(ngModel)]="selectedProjectInvoleID">
                                            <option value="" disabled selected hidden>--Chọn dự án--</option>
                                                       <option id="{{project.ProjectId}}" *ngFor="let project of lstProjectInvole" [value]="project.ProjectId">
                                            {{project.ProjectName}}</option>
                                                    </select>
                                </div>
                                <div class="col-lg-4 col-xs-12 frm-by-name-works">
                                    <div class="input-group" style="width: 100%">
                                        <input type="text" class="input-group form-control" [(ngModel)]="searchInvoleKeyword" placeholder="Tìm kiếm..." style="width: 65%">
                                        <select class="form-control" style="width: 35%" [(ngModel)]="searchTypeProjectInvole">
                                                       <option value="1">Tên công việc</option>
                                                       <option value="2">Người thực hiện</option>
                                                    </select>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="row row-short">
                                        <div class="col-lg-4 col-xs-12  dtmformdate">
                                            <div class="input-group">
                                                <form-datepicker id="txtInvoleFromDates" [(model)]=txtInvoleFromDates></form-datepicker>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 col-xs-12 dtmtodate">
                                            <div class="input-group">
                                                <form-datepicker id="txtInvoleToDates" placeholder="Đến ngày..." [(model)]=txtInvoleToDates></form-datepicker>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-xs-12 btn-search-works-related">
                                            <button type="button" class="btn btn btn-primary check-abc" (click)="SearchInvole()"> <i class="fa fa-search" aria-hidden="true"></i> Tìm kiếm</button>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="lstDetailGroup table-works-related">
                                <table class="table-hover" id="dataTable" style="width:100%">
                                    <thead>
                                        <th valign="middle" width="5%">
                                            <input type="checkbox" class="custom-checkbox" id="checkAll" name="all" [checked]="isAllChecked()" (change)="checkAll($event)"
                                            />
                                            <label for="checkAll"></label>
                                        </th>
                                        <th valign="middle" width="35%" align="left">Tên công việc</th>
                                        <th valign="middle" width="20%">Dự án</th>
                                        <th valign="middle" width="20%">Ngày bắt đầu</th>
                                        <th valign="middle" width="20%">Ngày kết thúc</th>

                                    </thead>
                                    <tbody>
                                        <tr *ngIf="listInvolee===null || listInvole.length===0">
                                            <td colspan="5" class="text-center">
                                                Không có công việc liên quan
                                            </td>
                                        </tr>
                                        <tr *ngFor="let works of listInvole">
                                            <td valign="middle" align="center">
                                                <input type="checkbox" class="custom-checkbox" id="{{works.WorksId}}" [(ngModel)]="works.state">
                                                <label [attr.for]="works.WorksId"></label>
                                            </td>
                                            <td valign="middle" align="left">{{works.WorksName}}</td>
                                            <td valign="middle">{{works.ProjectName}}</td>
                                            <td valign="middle" align="center">{{stringAsDate(works.StartDate)}}</td>
                                            <td valign="middle" align="center">{{stringAsDate(works.EndDate)}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <div class="form-group text-right btnActioninfo">
                                <button class="btn btn-eoffice green-bg" id="save" type="button" (click)="onSaveWorkInvole()"><i aria-hidden="true" class="fa fa-plus-square"></i> Thêm</button>
                                <button class="btn btn-eoffice white-bg " id="cancel" type="button" data-dismiss="modal"><i aria-hidden="true" class="fa fa-times"></i> Hủy</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modalAdd" aria-hidden="true">
                <div class="modal-dialog modal-lg popupTab" role="document">
                    <div class="modal-content">
                        <error-new-essage></error-new-essage>
                        <div class="titleProject">
                            <h2 class="modal-title">
                                <i class="fa fa-plus-square" aria-hidden="true"></i> Thêm công việc cần hỗ trợ
                            </h2>
                        </div>
                        <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
                        <div class="modal-body tab-scroll" style="padding: 20px;">
                            <ul class="nav nav-tabs tabs-page" role="tablist">
                                <li role="presentation" (click)="changeInforTab()" [class.active]="IsInformationTab" ><a href="#tab-works-info" aria-controls="tab-works-info" role="tab" data-toggle="tab">Thông tin</a></li>
                                <li role="presentation" (click)="changeInforTab()" [class.active]="!IsInformationTab"><a href="#tab-works-group-related" aria-controls="tab-works-group-related" role="tab" data-toggle="tab">Công việc liên quan</a></li>
                            </ul>
                            <div class="tab-content tabs-page">
                                <div role="tabpanel" [class.active]="IsInformationTab" class="tab-pane" id="tab-works-info">
                                    <div class="fg-table">
                                        <div class="dptable">
                                            <div class="dpColum lbl-width">
                                                <label>Tên công việc: <span style="color:red">*</span></label>
                                            </div>
                                            <div class="dpColum">
                                                <input (keyup)="validationPopupWork('name')" type="text" class="form-control" required [(ngModel)]="WorksSupportName" />
                                                <span class="alert-danger" *ngIf="ErrorWorksSupportName?.length >0">
                                                      {{ErrorWorksSupportName}}
                                                    </span>
                                            </div>
                                        </div>
                                        <div class="dptable">
                                            <div class="dpColum lbl-width">
                                                <label>Độ ưu tiên: <span style="color:red">*</span></label>
                                            </div>
                                            <div class="dpColum">
                                                <select (change)="validationPopupWork('sel')" class="form-control" required [(ngModel)]="selectedPriority">
                                                        <option value="" disabled selected hidden>--Chọn độ ưu tiên--</option>
                                                       <option id="{{priority.WorksSupportPriorityId}}" *ngFor="let priority of listPriority" [value]="priority.WorksSupportPriorityId">
                                                    {{priority.WorksSupportPriorityName}}
                                                    </option>
                                                    </select>
                                                <span class="alert-danger" *ngIf="ErrorSelPrioriry?.length >0">
                                                      {{ErrorSelPrioriry}}
                                                    </span>
                                            </div>
                                            <div class="dpColum lbl-width text-right">
                                                <label>Ngày dự kiến hoàn thành: </label>
                                            </div>
                                            <div class="dpColum">
                                                <form-datepicker minDate={{MinDate}} id="txtExpecDates" [(model)]=ExpectedCompletedDate></form-datepicker>
                                            </div>
                                        </div>
                                        <div class="dptable">
                                            <div class="dpColum lbl-width">
                                                <label>Nội dung: <span style="color:red">*</span></label>
                                            </div>
                                            <div class="dpColum">
                                                <textarea (keyup)="validationPopupWork('content')" type="text" class="form-control txtDesc" rows="5" required [(ngModel)]="Content"></textarea>
                                                <span class="alert-danger" *ngIf="ErrorContent?.length >0">
                                                      {{ErrorContent}}
                                                    </span>
                                            </div>
                                        </div>
                                        <div class="dptable">
                                            <div class="dpColum lbl-width">
                                                <label>Đính kèm: </label>
                                            </div>
                                            <div class="dpColum">
                                                <form-filesUpload (onFileUploaded)="onFileUploaded($event)" (onFileRemoved)="onFileRemoved($event)"></form-filesUpload>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div role="tabpanel" class="tab-pane" [class.active]="!IsInformationTab" id="tab-works-group-related">
                                    <div class="row row-short">
                                        <button data-toggle="modal" (click)="onAddWorksInvoleClick()" type="button" class="btn-eof blue-bg modal-add-works-group-related"><i class="fa fa-plus" aria-hidden="true"></i></button>
                                    </div>
                                    <div class="lstDetailGroup table-works-related">
                                        <table class="table-hover" id="dataTable" style="width:100%">
                                            <thead>
                                                <th valign="middle" width="35%" align="left">Tên công việc</th>
                                                <th valign="middle" width="20%">Ngày bắt đầu</th>
                                                <th valign="middle" width="20%">Ngày kết thúc</th>
                                                <th valign="middle" width="20%">Tiến độ</th>
                                                <th valign="middle" width="5%"></th>
                                            </thead>
                                            <tbody>
                                                <tr *ngIf="selectedListInvole==null || selectedListInvole.length===0">
                                                    <td colspan="5" class="text-center">
                                                        Không có công việc liên quan
                                                    </td>
                                                </tr>
                                                <tr *ngFor="let works of selectedListInvole">
                                                    <td valign="middle" align="left">{{works.WorksName}}</td>
                                                    <td valign="middle" align="center">{{stringAsDate(works.StartDate)}}</td>
                                                    <td valign="middle" align="center">{{stringAsDate(works.EndDate)}}</td>
                                                    <td valign="middle">
                                                        <div class="progress">
                                                            <div class="progress-bar" role="progressbar" [attr.aria-valuenow]="works.Currentprogress" aria-valuemin="0" aria-valuemax="100"
                                                                [ngStyle]="{'width': works.Currentprogress + '%'}">
                                                                <span class="sr-only">{{works.CurrentProgress}}% hoàn thành</span>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td valign="middle">
                                                        <button class="btn-eof red-bg" type="button" (click)="removeWorkInvole(works.WorksId)"><i class="fa fa-trash"></i></button>
                                                    </td>
                                                </tr>

                                            </tbody>
                                        </table>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <div class="form-group text-right btnActioninfo">
                                <button class="btn btn-eoffice green-bg" id="save" (click)="onAddWorks($event)" type="button"><i aria-hidden="true" class="fa fa-floppy-o"></i> Lưu</button>
                                <button class="btn btn-eoffice white-bg " id="cancel" type="button" data-dismiss="modal"><i aria-hidden="true" class="fa fa-times"></i> Hủy</button>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modalEdit">
                <div class="modal-dialog modal-lg popupTab" role="document">
                    <div class="modal-content">
                        <form (ngSubmit)="onEditWorks($event)">
                            <error-new-essage></error-new-essage>
                            <div class="titleProject">
                                <h2 class="modal-title">
                                    <i class="fa fa-pencil-square" aria-hidden="true"></i> Cập nhật công việc cần hỗ trợ
                                </h2>
                            </div>
                            <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
                            <div class="modal-body tab-scroll" style="padding: 20px;">
                                <ul class="nav nav-tabs tabs-page" role="tablist">
                                    <li role="presentation" (click)="changeInforTab()" [class.active]="IsInformationTab"><a href="#tab-works-info-edit" aria-controls="tab-works-info-edit" role="tab" data-toggle="tab">Thông tin</a></li>
                                    <li role="presentation" (click)="changeInforTab()" [class.active]="!IsInformationTab"><a href="#tab-works-group-related-edit" aria-controls="tab-works-group-related-edit"
                                            role="tab" data-toggle="tab">Công việc liên quan</a></li>
                                </ul>
                                <div class="tab-content tabs-page">
                                    <div role="tabpanel" class="tab-pane" id="tab-works-info-edit" [class.active]="IsInformationTab">
                                        <div class="fg-table">
                                            <div class="dptable">
                                                <div class="dpColum lbl-width">
                                                    <label>Tên công việc: </label>
                                                </div>
                                                <div class="dpColum">
                                                    <input type="text" (keyup)="validationPopupWork('name')" class="form-control" required [(ngModel)]="WorksSupportName" />
                                                    <span class="alert-danger" *ngIf="ErrorWorksSupportName?.length >0">
                                                      {{ErrorWorksSupportName}}
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="dptable">
                                                <div class="dpColum lbl-width">
                                                    <label>Độ ưu tiên:</label>
                                                </div>
                                                <div class="dpColum">
                                                    <select (change)="validationPopupWork('sel')" class="form-control" required [(ngModel)]="selectedPriority">
                                                        <option value="" disabled selected hidden>--Chọn độ ưu tiên--</option>
                                                       <option id="{{priority.WorksSupportPriorityId}}" *ngFor="let priority of listPriority" [value]="priority.WorksSupportPriorityId">
                                                    {{priority.WorksSupportPriorityName}}
                                                    </option>
                                                    </select>
                                                    <span class="alert-danger" *ngIf="ErrorSelPrioriry?.length >0">
                                                      {{ErrorSelPrioriry}}
                                                    </span>
                                                </div>
                                                <div class="dpColum lbl-width text-right">
                                                    <label>Ngày dự kiến hoàn thành: </label>
                                                </div>
                                                <div class="dpColum">
                                                    <form-datepicker id="txtExpecDates" [(model)]=ExpectedCompletedDate></form-datepicker>
                                                </div>
                                            </div>
                                            <div class="dptable">
                                                <div class="dpColum lbl-width">
                                                    <label>Nội dung: </label>
                                                </div>
                                                <div class="dpColum">
                                                    <textarea type="text" (keyup)="validationPopupWork('content')" class="form-control" required [(ngModel)]="Content"></textarea>
                                                    <span class="alert-danger" *ngIf="ErrorContent?.length >0">
                                                      {{ErrorContent}}
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="dptable">
                                                <div class="dpColum lbl-width">
                                                    <label>Đính kèm: </label>
                                                </div>
                                                <div class="dpColum">
                                                    <form-filesUpload (onFileUploaded)="onFileUploaded($event)" (onFileRemoved)="onFileRemoved($event)"></form-filesUpload>
