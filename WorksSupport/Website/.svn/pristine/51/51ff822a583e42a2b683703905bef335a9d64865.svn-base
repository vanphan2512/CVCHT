                                                                <i class="fa fa-file-text-o" aria-hidden="true"></i>
                                                                <span class="txtNameFile">{{file.FileName}}</span>
                                                            </div>
                                                            <a class="btn btn-delete" (click)="onDeleteUploadedFile(file.AttachmentId)">
