                    <li class="fa-hover col-md-3 col-sm-4">
                        <i class="fa fa-stethoscope"></i> stethoscope
                    </li>
                    <li class="fa-hover col-md-3 col-sm-4">
                        <i class="fa fa-user-md"></i> user-md
                    </li>
                    <li class="fa-hover col-md-3 col-sm-4">
                        <i class="fa fa-wheelchair"></i> wheelchair
                    </li>
                </ul>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i> Đóng</button>
            </div>
        </div>
    </div>
</div>
<div style="display:none">
    <div id="popover-content">
        <a (click)='onDelete($event)'>Xóa</a>
        <a (click)='onRename($event)'>Đổi tên</a>
        <a (click)='setInit($event)'>Là bước khởi tạo</a>
        <a (click)='setFinish($event)'>Là bước hoàn thành</a>
    </div>
</div>

<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="copyModel">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <div *ngIf='selectedCopyValue?.length > 0'>
                <h2 class="modal-title text-center">
                    Bạn có chắc chắn muốn sao chép!
                </h2>
            </div>
            <div *ngIf='selectedCopyValue?.length == 0'>
                <h2 class="modal-title text-center">
                    Vui lòng chọn dữ liệu để sao chép
                </h2>
            </div>

            <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>

            <div class="modal-body" *ngIf='selectedCopyValue?.length > 1'>
                <ul class="list-lg">
                    Vui lòng chọn 1 dòng để sao chép!
                </ul>
            </div>

            <div class="modal-body" *ngIf='selectedCopyValue?.length === 1'>
                <ul class="list-lg">
                    <li *ngFor="let member of selectedCopyValue">{{member.name}}</li>
                </ul>
            </div>
            <div *ngIf='selectedCopyValue?.length > 0' style="margin-top: 20px">
                <div class="text-center">
                    <button type="submit" class="btn btn-eoffice green-bg" (click)="onCopyWorksTypeClick($event)"><i class="fa fa-floppy-o" aria-hidden="true"></i> Sao chép</button>
                    <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i>  {{i18n.common.form.cancelDel}}</button>

                </div>
            </div>
            <div *ngIf='selectedCopyValue?.length == 0' style="margin-top: 20px">
                <div class="text-center">
                    <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i>  {{i18n.common.form.cancelDel}}</button>
                </div>
            </div>
        </div>
    </div>
</div>