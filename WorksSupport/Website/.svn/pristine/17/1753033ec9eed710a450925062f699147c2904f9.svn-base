<h2>
    {{i18n.configurations.worksType.title}}
</h2>
<check-status></check-status>
<div class="main-tab-content margin-top-15">
    <div class="nav-control">
        <div class="row">
            <div class="col-lg-6">
                <div class="main-buttons">
                    <button type="button" id="addNewWorkType" (click)="onAddNewWorkTypeClicked($event)" *ngIf="isCanAdd" class="btn-eof blue-bg m-right-10">
                        <i class="fa fa-plus" aria-hidden="true"></i>
                    </button>
                    <button type="button" id="btnDeletePopup" *ngIf="isCanDelete" (click)="onShowPopupDelete($event)" class="btn-eof red-bg m-right-10"
                        data-toggle="modal" data-target="#modal">
                        <i class="fa fa-trash-o" aria-hidden="true"></i>
                    </button>

                    <button type="button" id="btnDeletePopup" *ngIf="isCanAdd" (click)="onCopyClick($event)" class="btn-eof green-bg m-right-10"
                        data-toggle="modal" data-target="#copyModel">
                        <i class="fa fa-files-o" aria-hidden="true"></i>
                    </button>


                    <div class="main-search" id="searchButton">
                        <div class="input-group">
                            <input type="text" id="worksTypeName" name="worksTypeName" class="form-control txtInputSearch" placeholder="Nhập từ khóa tìm kiếm"
                                aria-describedby="basic-addon2">
                            <div class="input-group-addon" id="searchAddon" (click)="SearchByKey($event)">
                                <button type="button">
                                <i class="fa fa-search" aria-hidden="true"></i>
                            </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="tab-filter">
                    <div>
                        <div class="custom-select"><i class="fa fa-filter" aria-hidden="true"></i> Lọc theo:
                            <select class="form-control" (change)="onWorkTypeChange($event)">
                                <option value="0">Chưa xóa</option>
                                <option value="1">Đã xóa</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <grid [eventKey]="model.eventKey" (onToggleSelectChanged)="onToggleSelectChanged($event)" [options]="model.options" (onIconSelectChanged)="onIconSelectChanged($event)"
                (onItemEditClicked)="onEditWorkTypeClicked($event)"></grid>
        </div>
    </div>
</div>
<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modalCopy">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <h2 class="modal-title text-center">
                Thông báo
            </h2>
            <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
            <div class="modal-body">
                <div class="text-center">
                    <span *ngIf='selectedCopyValue?.length<1 || selectedCopyValue?.length>1'>Vui lòng chọn 1 dòng dữ liệu để sao chép!</span>
                    <span *ngIf='selectedCopyValue?.length === 1'>Sao chép thành công!</span>
                </div>
            </div>
            <div style="margin-top: 20px">
                <div class="text-center">
                    <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-check" aria-hidden="true"></i>  Đồng ý</button>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modal">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <div *ngIf='selectedValue?.length > 0'>
                <h2 class="modal-title text-center">
                    Bạn có chắc chắn muốn xóa!

                </h2>
            </div>
            <div *ngIf='selectedValue?.length == 0'>
                <h2 class="modal-title text-center">
                    Vui lòng chọn dữ liệu để xóa!
                </h2>
            </div>
            <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
            <div class="modal-body">
                <ul class="list-lg">
                    <li *ngFor="let member of selectedValue">{{member.name}}</li>
                </ul>
            </div>
            <div *ngIf='selectedValue?.length > 0' style="margin-top: 20px">
                <div class="text-center">
                    <button type="button" class="btn btn-eoffice red-bg" (click)="onDeleteWorkTypes($event)"><i class="fa fa-trash-o" aria-hidden="true"></i> Xóa</button>
                    <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i>  {{i18n.common.form.cancelDel}}</button>

                </div>
            </div>
            <div *ngIf='selectedValue?.length == 0' style="margin-top: 20px">
                <div class="text-center">
                    <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i>  {{i18n.common.form.cancelDel}}</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- start popup add -->
<div class="modal fade modal-create" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modalAdd">
    <div class="modal-dialog modal-lg modal-xlg" role="document">
        <div class="modal-content">
            <h2 class="modal-title"><i class="fa fa-plus-square" aria-hidden="true"></i> Thêm mới loại công việc cần hỗ trợ</h2>
            <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
            <div class="modal-body label-mar-right">
                <error-message *ngIf="IsAdd"></error-message>
                <div>
                    <table class="non-border">
                        <tr>
                            <td valign="middle" style="width:100px;"><label>Tên loại:</label></td>
                            <td valign="middle" [class.has-error]="ErrorTypeName"><input (keyup)="validErrorTypeName()" type="text" class="form-control m-bottom-5" [(ngModel)]=WorksSupportTypeName></td>
                        </tr>
                        <tr>
                            <td valign="middle">Mô tả:</td>
                            <td valign="middle" [class.has-error]="ErrorDescript"><input (keyup)="validErrorDescription()" id="Description" type="text" class="form-control m-bottom-5"
                                    [(ngModel)]=Description></td>
                        </tr>
                        <tr>
                            <td valign="middle">Biểu tượng</td>
                            <td valign="middle">
                                <table class="table" style="border-style: hidden;">
                                    <tr>
                                        <td style="width:40%">
                                            <span class="m-right-10"><i class="{{IconUrL}} fa-lg"></i>
                                                <button class="btn btn-eoffice green-bg" data-target="#myIconModal" data-toggle="modal" (click)="onIconSelected()" type="button"><i aria-hidden="true" class="fa fa-image"></i> Chọn biểu tượng</button> </span>
                                        </td>
                                        <td style="width:30%">
                                            <span class="valign">
                                    <input id="IsSystem" style="transform:translateY(3px);" [disabled]="!isAdmin" [checked]="IsSystem"
                                            (change)="IsSystem = !IsSystem" type="checkbox" class="custom-checkbox">
                                            <label for="IsSystem"> {{i18n.configurations.addWorksType.IsSystem}}</label>
                                    </span>
                                        </td>
                                        <td style="width:30%">
                                            <span class="m-right-10 valign">
                                        <span class="ios-tog ios-tog">
                                        <input type="checkbox" id="togglyIsActived" [(ngModel)]=IsActived>
                                        <label for="togglyIsActived"><i></i></label></span>
                                            <label for="IsCompleteStatus" class="lblIsActive">{{i18n.configurations.addWorksType.IsActived}}</label>
                                            </span>
                                        </td>
                                    </tr>
                                </table>


                            </td>
                        </tr>
                    </table>
                </div>

                <div class="row">
                    <div class="col-lg-6 col-sm-6 m-top-15">
                        <div class="small-tab">
                            <h3><span>Độ ưu tiên</span></h3>
                            <div class="small-tab-scroll">
                                <div class="m-bottom-5" *ngFor="let priority of listPriority">
                                    <input type="checkbox" class="cbPriority custom-checkbox" style="transform:translateY(3px);" attr.data-id="{{priority.WorksSupportPriorityId}}"
                                        id="priority-{{priority.WorksSupportPriorityId}}">
                                    <label attr.for="priority-{{priority.WorksSupportPriorityId}}">{{priority.WorksSupportPriorityName}}</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-sm-6 m-top-15">
                        <div class="small-tab">
                            <h3><span>Chất lượng</span></h3>
                            <div class="small-tab-scroll">

                                <div class="m-bottom-5" *ngFor="let quality of listQualities">
                                    <input type="checkbox" class="cbQualities custom-checkbox" style="transform:translateY(3px);" attr.data-id="{{quality.WorksSupportQualityId}}"
                                        id="quality-{{quality.WorksSupportQualityId}}">
                                    <label attr.for="quality-{{quality.WorksSupportQualityId}}">{{quality.WorksSupportQualityName}}</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12 small-tab m-top-15">
                    <h3><span>Quy trình xử lý</span></h3>
                    <ul id="ProcessStep" class="nav nav-tabs nav-stacked col-lg-3 col-sm-4 second-tab-nav" role="tablist">
                        <button type="button" class="btn btn-eoffice blue-bg m-top-15 m-bottom-15" (click)="addStep('edit')"><i class="fa fa-plus" aria-hidden="true"></i> Thêm mới</button>
                        <li role="presentation" *ngFor="let step of listStep" [ngClass]="{'active':currentStep===step.StepID,'':currentStep!==step.StepID}"
                            [attr.data-index]="r" id="{{step.StepID}}">
                            <a class="menu-popover" data-id="{{step.StepID}}">
                                                            <span id="Edit-Span-{{step.StepID}}" class="btn btn-eoffice white-bg m-right-10 addon" rel="popover" data-toggle="popover" role="button">
                                    <i
                                        class="fa fa-ellipsis-h" aria-hidden="true"></i>
                                        </span>
                                        </a>
                            <a class="ProcessStepA" aria-controls="btiepnhan" role="tab" data-toggle="tab" (click)="setCurrentStep($event)">
                                <div style="width: 100%" *ngIf="!step.isEdit" id={{step.StepID}} (click)="onTabClick($event)">
                                    <i class="fa fa-play fa-lg" *ngIf="step.StepISINITSTEP" style="color: #4caf50;" aria-hidden="true"></i>
                                    <i class="fa fa-power-off fa-lg" *ngIf="step.StepISFINISHSTEP" style="color: #ff3a3a;" aria-hidden="true"></i>                                    {{step.StepNAME}}
                                </div>
                                <span *ngIf="step.isEdit">
                                                                <input type="text" id="{{step.StepID}}" class="form-control input-sm m-top-10 m-right-10" required [(ngModel)]=step.StepNAME (keydown.enter)="onEnter($event)"/>
                                                                </span>
                            </a>
                        </li>
                    </ul>
                    <div class="col-lg-9 col-sm-8 second-tab tab-content" [hidden]="listStep.length === 0">
                        <div role="tabpanel" class="tab-pane fade in active" id="tabThongTin">
                            <h3 class="m-bottom-15"><span style="background:#f5f5f5">Thông tin bước xử lý</span></h3>
                            <div class="row">
                                <div class="col-lg-1 col-md-2">
                                    <label class="lb-form">Mô tả:</label>
                                </div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="m-bottom-5"><input type="text" class="form-control" [(ngModel)]=StepDESCRIPTION></div>
                                </div>
                            </div>
                            <div class="row row-short">
                                <div class="col-lg-1 col-md-2">
                                    <label class="lb-form">Mã màu:</label>
                                </div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="row">
                                        <div class="col-lg-4 col-md-4">
                                            <div class="m-bottom-5"><input type="text" class="form-control full-popover" readonly [ngStyle]="{'background-color': StepSTEPCOLORCODE}"
                                                    [value]="StepSTEPCOLORCODE" data-color-format="hex" /></div>
                                        </div>
                                        <div class="col-lg-4 col-md-8">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <label class="lb-form">Thời gian xử lý tối đa (giờ):</label>
                                                </div>
                                                <div class="col-md-5" [class.has-error]="ErrorMaxTime">
                                                    <div class="m-bottom-5"><input tabindex="forcus" #maxTime (keyup)="0" (keydown)="handleTimeMax(maxTime.value, $event)"
                                                            type="number" min="0" step="1" max="9999" class="form-control" [(ngModel)]=StepMAXPROCESSTIME></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row row-short">
                                <div class="col-lg-1 col-md-2">
                                    <label class="lb-form">Trạng thái:</label>
                                </div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="row">
                                        <div class="col-lg-4 col-md-4">
                                            <select class="form-control" [(ngModel)]="StepSTATUS">
                                                       <option id="{{status.WorksSupportStatusId}}" *ngFor="let status of listStatus" [value]="status.WorksSupportStatusId">
                                            {{status.WorksSupportStatusName}}</option>
                                                    </select>
                                        </div>
                                        <div class="col-lg-4 col-md-8">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <label class="lb-form">Phần trăm xử lý công việc:</label>
                                                </div>
                                                <div class="col-md-5">
                                                    <div class="m-bottom-6" [class.has-error]="ErrorStepProgress">
                                                        <input #timeProcess (keyup)="handleTimeProcess(timeProcess.value, $event)" type="number" min="0" step="1" max="100" class="form-control"
                                                            [(ngModel)]=WORKSSUPPORTSTEPPROGRESS />
                                                    </div>

                                                </div>
                                                <div style="float:left; margin-left:-10px; margin-top:8px">
                                                    %
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row row-short">
                                <div class="col-lg-1 col-md-2">
                                </div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="row">
                                        <div class="col-lg-5 col-md-5">

                                            <input [(ngModel)]="isMustChooseProcessUser" id="isMustChooseProcessUserAdd" type="checkbox" class="custom-checkbox" style="transform:translateY(3px);"
                                                (keyup)="0">
                                            <label for="isMustChooseProcessUserAdd">Bắt buộc chọn người xử lý</label>
                                        </div>
                                        <div class="col-lg-7 col-md-7">

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="m-bottom-15"></div>
                            <h3 class="m-top-15 m-bottom-15"><span style="background:#f5f5f5">Danh sách bước xử lý kế tiếp</span></h3>
                            <div class="row">
                                <div class="col-lg-1 col-md-2 hidden-sm hidden-xs">&nbsp;</div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="row selectedStep">
                                        <div *ngFor="let step of listStep">
                                            <div class="col-md-4" [hidden]="step.isEdit" *ngIf="currentStep!=step.StepID">
                                                <div class="m-bottom-5">
                                                    <input type="checkbox" value="{{step.StepID}}" id="selectedStepAdd{{step.StepID}}" [checked]="(listNextStep.indexOf(step.StepID))>-1"
                                                        (change)="onNextStepSelected($event)" class="custom-checkbox" style="transform:translateY(3px);">
                                                    <label attr.for="selectedStepAdd{{step.StepID}}">{{step.StepNAME}}</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div role="tabpanel" class="tab-pane fade" id="bxuly">
                            <h3><span style="background:#f5f5f5">Bước xử lý công việc</span></h3>
                        </div>
                        <div role="tabpanel" class="tab-pane fade" id="bhoanthanh">
                            <h3><span style="background:#f5f5f5">Bước hoàn thành</span></h3>
                        </div>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
            <div class="float-btn-btm">
                <div style="text-align:center">
                    <button type="submit" (click)="onAddClicked($event)" class="btn btn-eoffice green-bg"><i class="fa fa-floppy-o" aria-hidden="true"></i> Lưu</button>
                    <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i> Hủy</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end popup add -->
<!-- start popup edit -->
<div class="modal fade modal-create" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modalEdit">
    <div class="modal-dialog modal-lg modal-xlg" role="document">
        <div class="modal-content">
            <h2 class="modal-title"><i class="fa fa-pencil-square" aria-hidden="true"></i> Cập nhật loại công việc cần hỗ trợ</h2>
            <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
            <div class="modal-body label-mar-right">
                <error-message *ngIf="!IsAdd"></error-message>

                <div>
                    <table class="non-border">
                        <tr>
                            <td valign="middle" style="width:100px;"><label>Tên loại:</label></td>
                            <td valign="middle" [class.has-error]="ErrorTypeName"><input (keyup)="validErrorTypeName()" type="text" class="form-control m-bottom-5" required [(ngModel)]=WorksSupportTypeName></td>
                        </tr>
                        <tr>
                            <td valign="middle">Mô tả:</td>
                            <td valign="middle" [class.has-error]="ErrorDescript"><input (keyup)="validErrorDescription()" type="text" class="form-control m-bottom-5" [(ngModel)]=Description></td>
                        </tr>
                        <tr>
                            <td valign="middle">Biểu tượng:</td>
                            <td valign="middle">
                                <table style="border-style: hidden;">
                                    <tr>
                                        <td style="width:40%">
                                            <span class="m-right-10"><i class="{{IconUrL}}  fa-lg"></i> 
                            <button class="btn btn-eoffice green-bg" data-target="#myIconModal" data-toggle="modal" (click)="onIconSelected()" type="button"><i aria-hidden="true" class="fa fa-image"></i> Chọn biểu tượng</button>
                              </span>
                                        </td>
                                        <td style="width:30%">
                                            <span class="valign">
                                    <input class="custom-checkbox" id="IsSystem" style="transform:translateY(3px);" [disabled]="!isAdmin" [checked]="IsSystem"
                                            (change)="IsSystem = !IsSystem" type="checkbox"><label for="IsSystem"> {{i18n.configurations.addWorksType.IsSystem}}</label>
                                          
                                            </span>
                                        </td>
                                        <td style="width:30%">
                                            <span class="m-right-10 valign">
                                        <span class="ios-tog ios-tog">
                                        <input type="checkbox" id="togglyIsActived" [(ngModel)]=IsActived>
                                        <label for="togglyIsActived"><i></i></label></span>
                                            <label for="IsCompleteStatus" class="lblIsActive">{{i18n.configurations.addWorksType.IsActived}}</label>
                                            </span>
                                        </td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>
                </div>

                <div class="row">
                    <div class="col-lg-6 col-sm-6 m-top-15">
                        <div class="small-tab">
                            <h3><span>Độ ưu tiên</span></h3>
                            <div class="small-tab-scroll">
                                <div class="m-bottom-5" *ngFor="let priority of listPriority">
                                    <input type="checkbox" class="cbPriority custom-checkbox" style="transform:translateY(3px);" attr.data-id="{{priority.WorksSupportPriorityId}}"
                                        checked="{{listSelectedPriority.indexOf(priority.WorksSupportPriorityId)>-1}}" id="edpriority-{{priority.WorksSupportPriorityId}}">
                                    <label attr.for="edpriority-{{priority.WorksSupportPriorityId}}">{{priority.WorksSupportPriorityName}}</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-sm-6 m-top-15">
                        <div class="small-tab">
                            <h3><span>Chất lượng</span></h3>
                            <div class="small-tab-scroll">
                                <div class="m-bottom-5" *ngFor="let quality of listQualities">
                                    <input type="checkbox" class="cbQualities custom-checkbox" style="transform:translateY(3px);" attr.data-id="{{quality.WorksSupportQualityId}}"
                                        checked="{{listSelectedQualities.indexOf(quality.WorksSupportQualityId)>-1}}" id="edquality-{{quality.WorksSupportQualityId}}">
                                    <label attr.for="edquality-{{quality.WorksSupportQualityId}}">{{quality.WorksSupportQualityName}}</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="col-lg-12 small-tab m-top-15">
                    <h3><span>Quy trình xử lý</span></h3>
                    <ul id="ProcessStep" class="nav nav-tabs nav-stacked col-lg-3 col-sm-4 second-tab-nav" role="tablist">
                        <button type="button" class="btn btn-eoffice blue-bg m-top-15 m-bottom-15" (click)="addStep('edit')"><i class="fa fa-plus" aria-hidden="true"></i> Thêm mới</button>
                        <li role="presentation" *ngFor="let step of listStep" [ngClass]="{'active':currentStep===step.StepID,'':currentStep!==step.StepID}"
                            [attr.data-index]="r" id="{{step.StepID}}">
                            <a class="menu-popover" data-id="{{step.StepID}}">
                                                            <span id="Edit-Span-{{step.StepID}}" class="btn btn-eoffice white-bg m-right-10 addon" rel="popover" data-toggle="popover" role="button">
                                    <i
                                        class="fa fa-ellipsis-h" aria-hidden="true"></i>
                                        </span>
                                        </a>
                            <a class="ProcessStepA" aria-controls="btiepnhan" role="tab" data-toggle="tab" (click)="setCurrentStep($event)">
                                <div style="width: 100%" *ngIf="!step.isEdit" id={{step.StepID}} (click)="onTabClick($event)">
                                    <i class="fa fa-play fa-lg" *ngIf="step.StepISINITSTEP" style="color: #4caf50;" aria-hidden="true"></i>
                                    <i class="fa fa-power-off fa-lg" *ngIf="step.StepISFINISHSTEP" style="color: #ff3a3a;" aria-hidden="true"></i>                                    {{step.StepNAME}}
                                </div>
                                <span *ngIf="step.isEdit">
                                                                <input type="text" id="{{step.StepID}}" class="form-control input-sm m-top-10 m-right-10" required [(ngModel)]=step.StepNAME (keydown.enter)="onEnter($event)"/>
                                                                </span>
                            </a>
                        </li>
                    </ul>
                    <div class="col-lg-9 col-sm-8 second-tab tab-content" [hidden]="listStep.length === 0">
                        <div role="tabpanel" class="tab-pane fade in active" id="tabThongTin">
                            <h3 class="m-bottom-15"><span style="background:#f5f5f5">Thông tin bước xử lý</span></h3>
                            <div class="row">
                                <div class="col-lg-1 col-md-2">
                                    <label class="lb-form">Mô tả:</label>
                                </div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="m-bottom-5"><input type="text" class="form-control" [(ngModel)]=StepDESCRIPTION></div>
                                </div>
                            </div>
                            <div class="row row-short">
                                <div class="col-lg-1 col-md-2">
                                    <label class="lb-form">Mã màu:</label>
                                </div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="row">
                                        <div class="col-lg-4 col-md-4">
                                            <div class="m-bottom-5"><input type="text" class="form-control full-popover" readonly [ngStyle]="{'background-color': StepSTEPCOLORCODE}"
                                                    [value]="StepSTEPCOLORCODE" data-color-format="hex" /></div>
                                        </div>
                                        <div class="col-lg-4 col-md-8">
                                            <div class="row">
                                                <div class="col-md-5">
                                                    <label class="lb-form">Thời gian xử lý tối đa (giờ):</label>
                                                </div>
                                                <div class="col-md-6" [class.has-error]="ErrorMaxTime">
                                                    <div class="m-bottom-5"><input #maxTimeEdit (keyup)="handleTimeMax(maxTimeEdit.value, $event)"
                                                            type="number" required min="1" step="1" max="9999" class="form-control"
                                                            [(ngModel)]=StepMAXPROCESSTIME></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row row-short">
                                <div class="col-lg-1 col-md-2">
                                    <label class="lb-form">Trạng thái:</label>
                                </div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="row">
                                        <div class="col-lg-4 col-md-4">
                                            <div class="m-bottom-5">
                                                <select class="form-control" [(ngModel)]="StepSTATUS">
                                                       <option id="{{status.WorksSupportStatusId}}" *ngFor="let status of listStatus" [value]="status.WorksSupportStatusId">
                                            {{status.WorksSupportStatusName}}</option>
                                                    </select>
                                            </div>
                                        </div>

                                        <div class="col-lg-4 col-md-8">
                                            <div class="row">
                                                <div class="col-md-5">
                                                    <label class="lb-form">Phần trăm xử lý công việc:</label>
                                                </div>
                                                <div class="col-md-6" [class.has-error]="ErrorStepProgress">
                                                    <div class="m-bottom-5"><input type="number" #timeProcessEdit (keyup)="handleTimeProcess(timeProcessEdit.value, $event)"
                                                            min="1" required step="1" max="100" class="form-control" [(ngModel)]=WORKSSUPPORTSTEPPROGRESS></div>
                                                </div>
                                                <div style="float:left; margin-left:-10px; margin-top:8px">
                                                    %
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <div class="row row-short">
                                <div class="col-lg-1 col-md-2">
                                </div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="row">
                                        <div class="col-lg-5 col-md-5">

                                            <input [(ngModel)]="isMustChooseProcessUser" id="isMustChooseProcessUserEdit" type="checkbox" class="custom-checkbox" style="transform:translateY(3px);"
                                                (keyup)="0">
                                            <label for="isMustChooseProcessUserEdit">Bắt buộc chọn người xử lý</label>
                                        </div>
                                        <div class="col-lg-7 col-md-7">

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="m-bottom-15"></div>
                            <h3 class="m-top-15 m-bottom-15"><span style="background:#f5f5f5">Danh sách bước xử lý kế tiếp</span></h3>
                            <div class="row">
                                <div class="col-lg-1 col-md-2 hidden-sm hidden-xs">&nbsp;</div>
                                <div class="col-lg-11 col-md-10">
                                    <div class="row selectedStep">
                                        <div *ngFor="let step of listStep">
                                            <div class="col-md-4" [hidden]="step.isEdit" *ngIf="currentStep!=step.StepID">
                                                <div class="m-bottom-5">
                                                    <input type="checkbox" value="{{step.StepID}}" class="custom-checkbox" id="selectedStepEdit-{{step.StepID}}" [attr.data]="(listNextStep.indexOf(step.StepID))>-1"
                                                        [checked]="(listNextStep.indexOf(step.StepID))>-1" (change)="onNextStepSelected($event)"
                                                        style="transform:translateY(3px);">
                                                    <label attr.for="selectedStepEdit-{{step.StepID}}">{{step.StepNAME}}</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div role="tabpanel" class="tab-pane fade" id="bxuly">
                            <h3><span style="background:#f5f5f5">Bước xử lý công việc</span></h3>
                        </div>
                        <div role="tabpanel" class="tab-pane fade" id="bhoanthanh">
                            <h3><span style="background:#f5f5f5">Bước hoàn thành</span></h3>
                        </div>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
            <div class="float-btn-btm">
                <div style="text-align:center">
                    <button type="submit" (click)="onEditClicked($event)" class="btn btn-eoffice green-bg"><i class="fa fa-floppy-o" aria-hidden="true"></i> Lưu</button>
                    <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i> Hủy</button>
                </div>
            </div>
        </div>
    </div>
</div>
