                this.errorSolutionContent = "Nội dung giải pháp là bắt buộc";
                return false;
            } else {
                return true;
            }
        } else {
            if (this.solutionEditContent.trim() === "") {
                this.errorSolutionContent = "Nội dung giải pháp là bắt buộc";
                return false;
            } else {
                return true;
            }
        }
    }
