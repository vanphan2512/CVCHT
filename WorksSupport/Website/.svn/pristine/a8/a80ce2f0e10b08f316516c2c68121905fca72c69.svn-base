<div class="form-group">
    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">{{labelText}}</label>
    <div class="col-md-6 col-sm-6 col-xs-12">
        <select-permissions (onItemUnSelected)="onPermissionRemoved($event)" (onItemSelected)="onPermissionAdded($event)" [values]=model [placeHolder]=placeHolderText></select-permissions>
    </div>
</div>