<div class="main-content">
    <h2>
        {{i18n.eoffice.project.title}}
    </h2>
    <div class="main-tab-content label-mar-right">
        <div class="nav-control">
            <div class="row">
                <div class="col-lg-12 text-right">
                    <div class="main-buttons">
                        <div class="main-search m-right-10" id="searchButton">
                            <div class="input-group">
                                <input type="text" (keyup)="SearchByKey($event)" id="projectName" #projectName class="form-control" placeholder="Nhập từ khóa tìm kiếm"
                                    aria-describedby="basic-addon2">
                                <!-- <span class="input-group-addon" id="searchAddon" (click)="findProjectNameByKey(projectName.value)"><button type="button"><i class="fa fa-search" aria-hidden="true"></i></button></span> -->
                                <span class="input-group-addon" id="searchAddon" (click)="SearchByKey($event)"><button type="button"><i class="fa fa-search" aria-hidden="true"></i></button></span>

                            </div>
                        </div>
                        <button type="button" class="btn-eof blue-bg" id="addNewProject" *ngIf="lstWorksType?.length>0 || isAdmin" (click)="onAddNewProjectClick($event)"
                            data-toggle="modal" data-target="#modalUpdatePrpoject"><i class="fa fa-plus" aria-hidden="true"></i></button>
                    </div>
                </div>
            </div>
        </div>
        <div class="row list-all-project project-list ">
            <div *ngFor="let project of lstProject let i = index">
                <div class="col-lg-20p col-md-3 col-sm-4 col-xs-12 item text-center" *ngIf="!project.IsDeleted && (project.IsCanView || isAdmin)" [class.disactive]="project.IsActived == 0 ">
                    <div class="thumbnail">
                        <div class="row">
                            <div class="col-xs-6 text-left btn-detail-left">
                                <span>
                                <i class="fa fa-files-o" aria-hidden="true"></i>
                                    {{project.NumberOfWorkGroup}}
                                </span>
                                <span>
                                <i class="fa fa-user" aria-hidden="true"></i>
                                {{project.NumberOfMember}}
                            </span>
                            </div>
                            <div class="col-xs-6 text-right btn-r">
                                <span class="btn-detail-right text-center" *ngIf="((project.IsCanDelete || project.IsCanEdit) && !project.IsSystem) || isAdmin"> 
                                            <div class="dropdown dropdown-left">
                                                <i class="btn-ellipsis fa fa-ellipsis-h" aria-hidden="true" data-toggle="dropdown" id="menu1"></i>
                                               <ul class="dropdown-menu " role="menu" aria-labelledby="menu1">
                                                    <li *ngIf= "(project.IsCanEdit || isAdmin)">
                                                        <button data-toggle="modal" data-target="#modalUpdatePrpoject" type="button " class="btn dark-bg-drop" (click)="onUpdateProjectClick(project.WorksSupportProjectId)" ><i class="fa fa-pencil" aria-hidden="true"></i> Chỉnh sửa</button>  
                                                    </li>
                                                     <li (click)="checkProjectId(project.WorksSupportProjectId, project.WorksSupportProjectName)"class="chkIsDeleted"  id="{{project.WorksSupportProjectId}}"  *ngIf="(project.IsCanDelete || isAdmin)">
                                                        <button type="button" class="btn dark-bg-drop"   ><i class="fa fa-trash-o" aria-hidden="true"></i> Xóa</button>                                                       
                                                       
                                                    </li>
                                                </ul>
                                            </div>
                                        </span>
                            </div>
                        </div>


                        <a (click)="LoadWorksGroupByProjectId(project.WorksSupportProjectId, project.IsActived)">
                            <div class="folder-bg">
                                <i [attr.class]="project.IconUrl" aria-hidden="true"></i>
                            </div>
                        </a>
                    </div>
                    <div class="tit" title="{{project.WorksSupportProjectName}} ">

                        <a (click)="LoadWorksGroupByProjectId(project.WorksSupportProjectId, project.IsActived)">{{project.WorksSupportProjectName}} </a>
                    </div>
                </div>
            </div>
            <div class='col-xs-12 text-center show-error' style="display: none">Không có kết quả cho từ khóa này!</div>
        </div>
        <!--show popup  project-->
        <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modalUpdatePrpoject" data-backdrop="static">
            <div class="modal-dialog modal-lg popupTab" role="document" id="popupProject">
                <div class="modal-content">
                    <!--<form (ngSubmit)="onAddClicked($event)">-->
                    <div class="titleProject" class="titleProject">
                        <h2 class="modal-title" *ngIf="IsAddPopup">
                            <i class="fa fa-plus-square" aria-hidden="true"></i> Tạo dự án
                        </h2>
                        <h2 class="modal-title" *ngIf="!IsAddPopup">
                            <i aria-hidden="true" class="fa fa-pencil-square"></i> Cập nhật dự án
                        </h2>
                    </div>
                    <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
                    <div class="modal-body contentPopup">
                        <div>
                            <ul class="nav nav-tabs" role="tablist">
                                <li role="presentation" id="changeTag1" [class.active]=ShowProjectTab ><a (click)="checkTab()" href="#tabAddProject" aria-controls="tabAddProject" role="tab" data-toggle="tab">Thông tin</a></li>
                                <li  id="changeTag2" role="presentation" [class.active]=!ShowProjectTab ><a (click)="checkTab()" href="#memberlistpopup" aria-controls="member" role="tab"
                                        data-toggle="tab" id="clicktab">Thành viên ({{numberMemberOfProject}})</a></li>
                            </ul>
                            <div class="tab-content tab-scroll" id="myTabContent">
                                <div role="tabpanel" [class.active]=ShowProjectTab   class="tab-pane" id="tabAddProject">
                                    <!--<form-default>-->
                                    <error-new-essage></error-new-essage>
                                    <div class="fg-table">
                                        <div class="dptable">
                                            <div class="dpColum lbl-width">
                                                <label>Tên dự án: <span style = "color:red">*</span></label>
                                            </div>
                                            <div class="dpColum">

                                                <input class="form-control" required (keyup)="validationProjectName()" placeholder="Nhập tên dự án" title="Tên dự án" name="WorksSupportProjectName"
                                                    [(ngModel)]=WorksSupportProjectName />
                                                <span class="alert-danger" *ngIf="ErrorProjectName?.length >0">
                                                      {{ErrorProjectName}}
                                                </span>

                                            </div>
                                            <div class="dpColum lbl-width text-right">
                                                <label>Loại công việc: <span style="color:red">*</span></label>
                                            </div>
                                            <div class="dpColum">
                                                <select class="form-control" title="Loại công việc" [disabled]="!IsAddPopup" (change)="reloadMemberRoleList(typeIdOfProct.value)"
                                                    #typeIdOfProct [(ngModel)]=WorksSupportTypeId required>   
                                                                                            <option value="">--Chọn loại công việc--</option>
                                                                                            <option *ngFor="let obj of lstWorksType" [attr.value]=obj.WorksSupportTypeId [attr.selected]="obj.WorksSupportTypeId == WorksSupportTypeId ? true : null">
                                                                                                {{obj.WorksSupportTypeName}}
                                                                                            </option>  

                                                </select>
                                                <span class="alert-danger" *ngIf="ErrorWorksSupportTypeId?.length > 0">
                                                {{ErrorWorksSupportTypeId}}
                                                </span>
                                            </div>
                                        </div>
                                        <div class="dptable">
                                            <div class="dpColum lbl-width">
                                                <label>Mô tả: </label>
                                            </div>
                                            <div class="dpColum">
                                                <form-textarea [placeHolderText]=i18n.eoffice.addProject.inputDesc [(model)]=Description>
                                                </form-textarea>

                                                <span class="alert-danger" *ngIf="ErrorDescription?.length >0">
                                                      {{ErrorDescription}}
                                                </span>
                                            </div>
                                        </div>
                                        <!-- row 3-->
                                        <div class="dptable">
                                            <div class="dpColum lbl-width"></div>
                                            <div class="dpColum">
                                                <!--<a href="" class="btn btn-primary m-right-10" *ngIf="IconUrl != null"> -->
                                                <span class="btn btn-primary m-right-10" *ngIf="IconUrl != null"> 
                                            <i class="{{IconUrl}}"></i>                                     
                                                    </span>
                                                <a class="btn-link" data-toggle="modal" data-target="#myIconModal" (click)="onIconSelectedAdd()">
                                            <i class="fa fa-caret-square-o-down" aria-hidden="true"></i> {{i18n.configurations.addOrUpdatePriority.selectIcon}}
                                        </a>
                                            </div>
                                        </div>
                                        <div class="dptable">
                                            <div class="dpColum lbl-width"></div>
                                            <div class="dpColum lbl-width">
                                                <div class="btnGroupSystem">
                                                    <input type="checkbox" [disabled]="!isAdmin" class="custom-checkbox" [(ngModel)]=IsSystem id="IsSystem">
                                                    <label for="IsSystem">Hệ thống</label>
                                                </div>
                                            </div>
                                            <div class="dpColum">
                                                <span class="ios-tog ios-tog"> 
                                                                                            <input type="checkbox" id="togglyIsActived" [(ngModel)]=IsActived>
                                                                                            <label for="togglyIsActived"><i></i></label>
                                                                                        </span>
                                                <label for="IsSystem" class="lblActive"> Kích hoạt</label>
                                            </div>
                                        </div>
                                    </div>
                                    <!--</form-default>-->
                                </div>
                                <div role="tabpanel" [class.active]=!ShowProjectTab  class="tab-pane" id="memberlistpopup">
                                    <div class="nav-control nav-inside">
                                        <div class="row">
                                            <div class="col-xs-6">
                                                <div class="main-filter">
                                                    <div class="dpTable">
                                                        <div class="dpColum">
                                                            <label>Xem theo: </label>
                                                        </div>
                                                        <div class="dpColum" id="set-select">
                                                            <select class="form-control" (change)="selectShowRoleProject($event)" [(ngModel)]=ShowRoleProject>
                                                                <option *ngFor="let role of LstShowRoleProject" [attr.value]="role.roleId" >{{role.roleName}}</option>                                                               
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xs-6 text-right">
                                                <div class="main-buttons">
                                                    <div class="main-search m-right-10 active">
                                                        <div class="input-group">
                                                            <input type="text" id="txt-search-member" (keyup)="findMemberProject()" [(ngModel)]=KeySearchMember class="form-control"
                                                                placeholder="Nhập từ khóa tìm kiếm" aria-describedby="basic-addon2">
                                                            <span class="input-group-addon" id="searchAddon-member" (click)="findMemberProject()">
                                                                <button  type="button" ><i class="fa fa-search" aria-hidden="true"></i></button>
                                                            </span>
                                                        </div>
                                                    </div>
                                                    <button type="button" (click)="showPopupSearchMember($event)" class="btn-eof blue-bg" data-toggle="modal" data-target="#modalMember">
                                                        <i class="fa fa-plus" aria-hidden="true"></i>
                                                    </button>
                                                </div>
                                            </div>


                                        </div>
                                    </div>
                                    <!-- all member -->
                                    <div class="member-list" *ngIf="IsRole">
                                        <div class="member-group">
                                            <div class="row">
                                                <div *ngFor="let member of lstShowAllMember">
                                                    <div class="col-lg-2 col-sm-3 col-xs-6" *ngIf="!member.IsChecked">
                                                        <div class="member-item">
                                                            <div class="btn-detail">
                                                                <span class="btn-detail-left">
                                                                    <i class="fa " aria-hidden="true" [class.fa-star-o]="member.IsAutoAddMemberToWorksGroup == 0"
                                                                     [class.fa-star]="member.IsAutoAddMemberToWorksGroup == 1" (click)="ClickUserAuto(member.UserName)">
                                                                        </i>
                                                                        </span>
                                                                <span class="btn-detail-right"><i class="fa fa-trash-o" aria-hidden="true"(click)="removeMemberInMemberList(member.UserName)"></i></span>
                                                            </div>
                                                            <div class="avatar-circle">
                                                                <img src="http://eofficecnd.nc.com/album/{{member.DefaultPictureURL}}" alt="{{member.FullName}}">
                                                            </div>
                                                            <div class="member-info">
                                                                <span class="member-name">{{member.FullName}}</span>
                                                                <span class="member-department">{{member.DepartmentName}}</span>
                                                            </div>
                                                            <select class="form-control" #memberRoleAll (change)="changeMemberRole(memberRoleAll.value, member.UserName, member.WorksSupportMemberRoleId, memberRoleAll.text)">                                                            
                                                             <option *ngFor="let obj of lstMemberRole"  
                                                              value={{obj.WorksSupportMemberRoleId}} [attr.selected]="obj.WorksSupportMemberRoleId == member.WorksSupportMemberRoleId ? true : null" >  
                                                              {{obj.WorksSupportMemberRoleName}}
                                                             </option>
                                                        </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--role member -->
                                    <div class="member-group" *ngIf="!IsRole  ">
                                        <div *ngFor="let role of lstShowMember">
                                            <h3 class="member-group-title" *ngIf="role.count > 0">{{role.WorksSupportMemberRoleName}} ({{role.count}}) </h3>
                                            <div class="row">
                                                <!--{{role.NumberOfMembers}}-->
                                                <div *ngFor="let member of role.lstMember">
                                                    <div class="col-lg-2 col-sm-3 col-xs-6" *ngIf="!member.IsChecked">
                                                        <div class="member-item">
                                                            <div class="btn-detail">
                                                                <div class="btn-detail-left">
                                                                    <i class="fa" aria-hidden="true" [class.fa-star-o]="member.IsAutoAddMemberToWorksGroup == 0" [class.fa-star]="member.IsAutoAddMemberToWorksGroup == 1"
                                                                        (click)="ClickUserAuto(member.UserName)">
                                                                        </i>
                                                                </div>
                                                                <span class="btn-detail-right"><i class="fa fa-trash-o" aria-hidden="true"  (click)="removeMemberInMemberList(member.UserName)"></i></span>
                                                            </div>
                                                            <div class="avatar-circle">
                                                                <img src="http://eofficecnd.nc.com/album/{{member.DefaultPictureURL}}" alt="{{member.FullName}}">
                                                            </div>
                                                            <div class="member-info">
                                                                <span class="member-name">{{member.FullName}}</span>
                                                                <span class="member-department">{{member.DepartmentName}}</span>
                                                            </div>
                                                            <select class="form-control" #memberRole (change)="changeMemberRole(memberRole.value, member.UserName, member.WorksSupportMemberRoleId, memberRole.text)">                                                            
                                                             <option *ngFor="let obj of lstMemberRole"  
                                                              value={{obj.WorksSupportMemberRoleId}} [attr.selected]="obj.WorksSupportMemberRoleId == member.WorksSupportMemberRoleId ? true : null" >  
                                                              {{obj.WorksSupportMemberRoleName}}
                                                             </option>
                                                        </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div *ngIf="SearchMemberInfor" class='col-xs-12 text-center show-error'>Không có kết quả cho từ khóa này!</div>

                                    <div class="note-member m-top-15"><i class="fa fa-star text-warning" aria-hidden="true"></i> <label>Thành viên nguồn dự án</label></div>
                                </div>

                            </div>

                        </div>
                        <div class="modal-footer">
                            <div class="text-right btnActioninfo">
                                <button id="saveProject" type="button" (click)="onAddClicked($event)" class="btn btn-eoffice green-bg"><i class="fa fa-floppy-o" aria-hidden="true"></i> {{i18n.common.form.save}}</button>

                                <button type="button" data-dismiss="modal" id="cancelProject" class="btn btn-eoffice white-bg"><i aria-hidden="true" class="fa fa-times"></i> Hủy</button>

                            </div>
                        </div>
                    </div>
                    <!--</form>-->
                </div>
            </div>

        </div>
        <!--show popup project member -->
        <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="modalMember" data-backdrop="static">
            <div class="modal-dialog modal-lg popupTab" role="document" id="popupProjectAdd">
                <div class="modal-content member-add">
                    <div class="titleProject">
                        <h2 class="modal-title">
                            <i class="fa fa-user-plus" aria-hidden="true"></i> Thêm thành viên dự án
                        </h2>
                    </div>
                    <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
                    <!--<button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>-->
                    <div class="modal-body">
                        <div class="member-add-top">
                            <div class="row">
                                <div class="col-sm-8">
                                    <div class="row row-short">
                                        <div class="col-lg-4">
                                            <select id="selDepartment" class="form-control WorksSupportDepartment" [(ngModel)]=WorksSupportDepartmentId>  
                                        <option value="-1">--Chọn phòng ban--</option>  
                                        <option *ngFor="let obj of lstDepartment"
                                                value={{obj.DepartmentId}}>  
                                                {{obj.DepartmentName}}
                                        </option>  
                                    </select>
                                        </div>
                                        <div class="col-lg-4">
                                            <div id="imaginary_container">
                                                <div class="input-group stylish-input-group">
                                                    <input type="text" (keyup)="SearchUserByKey($event)" id="txtKeyNameUser" name="btnBaseUser" class="form-control" placeholder="Nhập từ khóa tìm kiếm"
                                                        [(ngModel)]=txtKeyNameUser>
                                                    <span class="input-group-addon" (click)="SearchUserByKey($event)" id="searchAddon-AddMember">
                                                        <button type="submit" >
                                                            <span class="fa fa-search"></span>
                                                    </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4 text-right">
                                    <div class="frmbtn-group">
                                        <div class="btnGroupSystem">
                                            <!--<input type="checkbox" class="custom-checkbox" [(ngModel)]=isCheckAll id="lstAllMember" (click)="SelectUserName($event)">-->
                                            <input type="checkbox" class="custom-checkbox" [(ngModel)]=isCheckAll id="lstAllMember" (click)="SelectUserName($event)">
                                            <label for="lstAllMember">Chọn tất cả</label>
                                        </div>
                                    </div>

                                </div>
                            </div>

                        </div>
                        <div class="member-list tab-scroll">
                            <div class="member-group">
                                <div class="row">
                                    <div class="col-lg-2 col-sm-3 col-xs-6" *ngFor="let obj of lstMember">
                                        <div class="member-item " [class.selected]="obj.MemberUserName !== null" (click)="ClickUser(obj.UserName)">
                                            <div class="btn-detail">
                                                <span class="btn-detail-right">
                                                    <i aria-hidden="true" class="fa fa-circle-o" aria-hidden="true"  id ={{obj.UserName}} style="color: #0288d1;padding-left: 5px;padding-top: 3px;">                                  
                                                    </i>
                                                </span>
                                            </div>
                                            <div class="avatar-circle">
                                                <img src="http://eofficecnd.nc.com/album/{{obj.DefaultPictureURL}}" alt="{{obj.FullName}}">
                                            </div>
                                            <div class="member-info">
                                                <span class="member-name">{{obj.FullName}}</span>
                                                <span class="member-department">{{obj.DepartmentName}}</span>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <!-- show message not found member -->
                            <div *ngIf="SearchMemberPopup" class='col-xs-12 text-center show-error'>Không có kết quả cho từ khóa này!</div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <div class="form-group text-right btnActioninfo">
                            <button (click)="addNewMemberProject($event)" class="btn btn-eoffice green-bg" id="save" type="button"><i aria-hidden="true" class="fa fa-plus-square"></i> Thêm</button>
                            <button class="btn btn-eoffice white-bg " data-dismiss="modal" id="cancelMember" type="button"><i aria-hidden="true" class="fa fa-times"></i> Hủy</button>

                        </div>
                    </div>
                </div>
            </div>

        </div>
        <!--Show popup Delete  project-->
        <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" id="ModalDelete" data-backdrop="static">
            <div class="modal-dialog modal-md" role="document">
                <div class="modal-content">
                    <div *ngIf='selectedValue?.length > 0'>
                        <h2 class="modal-title text-center">
                            Bạn có chắc chắn muốn xóa?

                        </h2>
                    </div>
                    <div *ngIf='selectedValue?.length == 0'>
                        <h2 class="modal-title text-center">
                            Vui lòng chọn dữ liệu để xóa!
                        </h2>
                    </div>
                    <button type="button" class="close close-modal" data-dismiss="modal" aria-label="Close"><i class="fa fa-times-circle-o" aria-hidden="true"></i></button>
                    <div class="modal-body">
                        <!-- <ul class="list-lg">
                            <li *ngFor="let member of selectedValue">{{member.name}}</li>
                        </ul> -->
                    </div>
                    <div *ngIf='selectedValue?.length > 0'>
                        <div class="text-center">
                            <button type="button" class="btn btn-eoffice red-bg" (click)="onDeleteProjects($event)"><i class="fa fa-trash-o" aria-hidden="true"></i> Xóa</button>
                            <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i> Bỏ qua</button>

                        </div>
                    </div>
                    <div *ngIf='selectedValue?.length == 0'>
                        <div class="text-center">
                            <button type="button" class="btn btn-eoffice white-bg m-right-10" data-dismiss="modal" aria-label="Close"><i class="fa fa-times" aria-hidden="true"></i> Bỏ qua</button>
                        </div>
                    </div>

                </div>
            </div>
        </div>

        <!--Show popup icon-->
        <div class="modal fade" id="myIconModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="false" data-backdrop="static">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                        <h4 class="modal-title">Danh sách biểu tượng</h4>
                    </div>
                    <div class="modal-body tab-scroll">
                        <ul class="row the-icons">
                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-address-book"></i> address-book</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-address-book-o"></i> address-book-o</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-address-card"></i> address-card</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-address-card-o"></i> address-card-o</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-bandcamp"></i> bandcamp</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-bath"></i> bath</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-bathtub"></i> bathtub</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-drivers-license"></i> drivers-license</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-drivers-license-o"></i> drivers-license-o</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-eercast"></i> eercast</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-envelope-open"></i> envelope-open</li>

                            <li class="fa-hover col-md-3 col-sm-4"><i class="fa fa-envelope-open-o"></i> envelope-open-o</li>

