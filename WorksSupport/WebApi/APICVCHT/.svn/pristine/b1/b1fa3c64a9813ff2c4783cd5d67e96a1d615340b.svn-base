                        IsCanDeleteWorksSupport = itemMemberRole.IsCanDeleteWorksSupport,
                        IsDefaultRole = itemMemberRole.IsDefaultRole
                    };

                    daWorksSupportTypeMemberRole.InsertMemberRole(newItemMemberRole, objIData);
