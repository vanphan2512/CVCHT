CREATE OR REPLACE PROCEDURE ERP.EO_Project_V2_SHR
/*
	CREATED BY	:	NGUYEN THI KIM NGAN
	DATE		:	06.07.2017
	DESCRIPTION	:	Load du an 
*/
(
  V_OUT OUT SYS_REFCURSOR
)
AS
BEGIN
  OPEN V_OUT FOR
   SELECT 
    EO_PROJECT.PROJECTID,
    EO_PROJECT.PROJECTNAME
    FROM EO_PROJECT 
    WHERE ISDELETED = 0;
END;
/