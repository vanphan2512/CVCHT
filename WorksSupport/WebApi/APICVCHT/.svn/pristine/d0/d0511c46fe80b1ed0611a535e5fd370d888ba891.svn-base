create or replace PROCEDURE EO_WORKSSUPPORT_V2_QUALITY
/*
	CREATED BY	:	NGUYEN THI KIM NGAN
	DATE		:	08/06/2017
	DESCRIPTION	:	C?P NH?T THÔNG TIN NHÓM CÔNG VI?C C?N H? TR? 
*/ (
    V_WORKSSUPPORTID            IN EO_WORKSSUPPORT.WORKSSUPPORTID%TYPE,
    V_WORKSSUPPORTQUALITYID     IN EO_WORKSSUPPORT.WORKSSUPPORTQUALITYID%TYPE,
    V_QUALITYNOTE               IN EO_WORKSSUPPORT.WORKSSUPPORTQUALITYNOTE%TYPE,
    V_UPDATEDUSER               IN EO_WORKSSUPPORT.UPDATEDUSER%TYPE
)
    AS
BEGIN
    UPDATE EO_WORKSSUPPORT
        SET
            WORKSSUPPORTQUALITYID = V_WORKSSUPPORTQUALITYID,
            WORKSSUPPORTQUALITYNOTE = V_QUALITYNOTE,
            UPDATEDUSER = V_UPDATEDUSER,
            UPDATEDDATE = SYSDATE
    WHERE
        WORKSSUPPORTID = V_WORKSSUPPORTID;
END;