CREATE OR REPLACE PROCEDURE EO_WORKSSUPPORTTYPE_WF_SEL
/*
	CREATED BY	:	NGUYEN THI KIM NGAN
	DATE		:	02/03/2013
	DESCRIPTION	:	N?P THÔNG TIN BUOC XU LY CÔNG VI?C C?N H? TR? 
*/ (
    V_WORKSSUPPORTTYPEID   IN EO_WORKSSUPPORTTYPE_WORKFLOW.WORKSSUPPORTTYPEID%TYPE,
    V_OUT                  OUT SYS_REFCURSOR
)
    AS
BEGIN
    OPEN V_OUT FOR
        SELECT
            WORKSSUPPORTSTEPID,
            WORKSSUPPORTTYPEID,
            WORKSSUPPORTSTEPNAME,
            UPDATEDUSER,
            UPDATEDDATE,
            PROCESSFUNCTIONID,
            STEPCOLORCODE,
            MAXPROCESSTIME,
            ISINITSTEP,
            ISFINISHSTEP,
            ISMUSTCHOOSEPROCESSUSER,
            AUTOCHANGETOSTATUSID,
            DESCRIPTION,
            ORDERINDEX,
            ISACTIVE,
            ISSYSTEM,
            ISCANMODIFIEDCONTENT,
            ISCANMODIFIEDCONTENTSOLUTION,
            ISCANMODIFIEDCOMPLETEDDATE,
            ISCANATTACH,
            ISCANCOMMENT,
            ISCANUPDATEPROCESS,
            ISCANUPDATEQUALITY,
            WORKSSUPPORTSTATUSID
        FROM
            EO_WORKSSUPPORTTYPE_WORKFLOW
        WHERE
            WORKSSUPPORTTYPEID = V_WORKSSUPPORTTYPEID;

END;