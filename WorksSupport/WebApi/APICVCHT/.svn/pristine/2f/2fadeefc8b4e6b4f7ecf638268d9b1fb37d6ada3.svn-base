create or replace PROCEDURE EO_PROJECT_V2_SHR
/*
	CREATED BY	:	NGUYEN THI KIM NGAN
	DATE		:	06.07.2017
	DESCRIPTION	:	LOAD DU AN 
*/
(
  V_OUT OUT SYS_REFCURSOR
)
AS
BEGIN
    OPEN V_OUT FOR
    SELECT 
        EO_PROJECT.PROJECTID,
        EO_PROJECT.PROJECTNAME
    FROM 
        EO_PROJECT 
    WHERE 
        ISDELETED = 0;
END;
