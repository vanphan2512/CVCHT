.ios-tog {
    width: 50px;
    display: inline-block;
    text-align: left;
}
.ios-tog label {
  display: block;
  width: 50px;
  height: 22px;
  margin: 0px auto;
  border-radius: 100px;
  transition: all 0.2s ease-in-out;
  -webkit-transition: all 0.2s ease-in-out;
  background-color: #c5c3c6;
}

.ios-tog input {
  display: none;
}
.ios-tog i {
  height: 20px;
  width: 20px;
  background: #ffffff;
  display: inline-block;
  border-radius: 100px;
  margin-top: 1px;
  margin-left: 0;
  transition: all 0.2s ease-in-out;
  -webkit-transition: all 0.2s ease-in-out;
  pointer-events: none;
  box-shadow: 0px 0px 0px 0px rgba(0, 0, 0, 0);
}
.ios-tog label:hover>i {
  box-shadow: 0px 1px 2px 0px rgba(0, 0, 0, 0.20);
  transform: scale(1.2);
}

.ios-tog input[type=checkbox] + label {
  display:inline-block;
}
.ios-tog input:checked+label>i {
  margin-left: 29px;
}

.ios-tog label:active {
  background-color: #c5c3c6;
}
.ios-tog label:active>i {
  width: 34px;
  box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.20);
}
.ios-tog input:checked+label:active>i {
  margin-left: 18px;
}
.ios-tog input:checked+label {
  background-color: #0288d1;
}
.ios-tog-danger input:checked+label {
  background-color: #dd2c00;
}
.ios-tog-info input:checked+label {
  background-color: #ffd180;
}
.ios-tog-success input:checked+label {
  background-color: #4caf50;
}
