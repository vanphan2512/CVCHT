.marquee {
    width: 100%;
    margin: 0 auto;
    white-space: nowrap;
    overflow: hidden;
    box-sizing: border-box;
  }
  
  .marquee a {
    display: inline-block;
    padding-left: 0%;
    text-indent: 0;
  }
  .marquee a:hover {
    animation: marquee 10s linear infinite;
  }
  .marquee p {
    float: left;
    width: 100%;
    line-height: 30px;
  }

  @keyframes marquee {
    0%   { transform: translate(0, 0); }
    100% { transform: translate(-100%, 0); }
  }

